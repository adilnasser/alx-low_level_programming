#include "main.h"

/**
 * _strcat- This function appends the src string to the dest string
 * @dest:string
 * @src:string
 * Returns-@dest  pointer to the resulting string dest
 */

char *_strcat(char *dest, char *src)
{

	int lenghtdest = 0, lenghtsrc = 0;

	int i = 0, j = 0;

	while (dest[lenghtdest] != '\0')
	{
		lenghtdest++;
	}

	while (src[lenghtsrc] != '\0')
	{
		lenghtsrc++;
	}

	for (i = lenghtdest + 1; i <= lenghtdest + 1 ; i++)
	{
		while (j < lenghtsrc)
		{
			*(dest + i) = *(src + j);
			j++;
		}
	}
	return (dest);
}
